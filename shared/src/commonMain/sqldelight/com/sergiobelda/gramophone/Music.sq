selectAllSongs:
SELECT *
FROM DbSong;

selectSongById:
SELECT *
FROM DbSong
WHERE id = ?;

selectAllArtists:
SELECT *
FROM DbArtist;

selectArtistById:
SELECT *
FROM DbArtist
WHERE id = ?;

selectAllAlbums:
SELECT *
FROM DbAlbum;

selectAlbumById:
SELECT *
FROM DbAlbum
WHERE id = ?;
