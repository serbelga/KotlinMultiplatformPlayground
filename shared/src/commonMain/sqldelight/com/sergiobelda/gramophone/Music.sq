CREATE TABLE DbSong (
    id INTEGER PRIMARY KEY,
    title TEXT NOT NULL
);

CREATE TABLE DbArtist (
    id INTEGER PRIMARY KEY,
    name TEXT NOT NULL
);

CREATE TABLE DbAlbum (
    id INTEGER PRIMARY KEY,
    title TEXT NOT NULL,
    release_year INTEGER NOT NULL,
    artist_id INTEGER NOT NULL,
    FOREIGN KEY(artist_id) REFERENCES DbArtist(id)
);

